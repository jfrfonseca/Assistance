phantom.injectJs('peer.js')

//peerA = new Peer({ key: 'zn0y9k200kgtx1or', debug: 3});

//Once, I got the ID 0w6bsifcgfjhh0k9
peerA = new Peer('0w6bsifcgfjhh0k9');

peerA.on('open', function(id) {
  console.log('My peer ID is: ' + id);
});

//Create Connection to the ID of PeerB
var conn = peerA.connect('xexvzkf7bv8wipb9');

conn.on('open', function() {
  console.log('Connection Established');

  // Send messages
  console.log('Sent');
  conn.send('Hello!');
});
