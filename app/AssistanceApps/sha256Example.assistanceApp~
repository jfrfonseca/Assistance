#!/bin/bash
currentDirectory=$(dirname $0)
processPriority=$1
dataFiles=$2
serviceTicket=$3
arguments=$4
nice -n $processPriority time -p ./$currentDirectory/sha256/V0.5/linux/bin/main.build $dataFiles $arguments >> $currentDirectory/outputs/$serviceTicket-output.dat 2> $currentDirectory/outputs/$serviceTicket-errors.dat
