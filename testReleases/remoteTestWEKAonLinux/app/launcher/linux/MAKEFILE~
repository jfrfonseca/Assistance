CC 		= g++
CFLAGS		= -O2
BINLOC		= bin
SRCLOC		= ../../src
TSTLOC		= ../../tests
FUNCNAME	= Assistance.sha256

all: clean ${FUNCNAME}

${FUNCNAME}:  main.cpp 
	${CC} ${CFLAGS} ${SRCLOC}/main.cpp -o ${BINLOC}/main.build 

test0:  
	time -p ./${BINLOC}/main.build ${TSTLOC}/test0data.dat 1000

clean:
	 rm -f ${BINLOC}/*.o ${BINLOC}/*.build 

